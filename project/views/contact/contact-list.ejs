<h1><span>contact requests</span></h1>
<table>

	<thead>
		<tr>
			<th>Date</th>
			<th>Name</th>
			<th>Message</th>
			<th></th>
		</tr>
	</thead>
	<tbody>

		<% contacts.forEach((contact)=> { %>
			<tr>
				<td>
					<%= dateFormat(contact.datePosted, "mediumDate" ) %>
				</td>
				<td>
					<%= contact.name %>
				</td>
				<td>
					<%= contact.shortMessage %>
				</td>
				<td><a href="/contacts/<%= contact._id %>/edit" class="button">View</a></td>
			</tr>

			<% }) %>

	</tbody>
</table>